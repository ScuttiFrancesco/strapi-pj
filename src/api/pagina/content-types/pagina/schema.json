{
  "kind": "collectionType",
  "collectionName": "paginas",
  "info": {
    "singularName": "pagina",
    "pluralName": "paginas",
    "displayName": "Pagina"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "_softDeletedAt": {
      "type": "datetime",
      "configurable": false,
      "writable": false,
      "visible": false,
      "private": true
    },
    "_softDeletedById": {
      "type": "integer",
      "configurable": false,
      "writable": false,
      "visible": false,
      "private": true
    },
    "_softDeletedByType": {
      "type": "string",
      "configurable": false,
      "writable": false,
      "visible": false,
      "private": true
    },
    "titolo": {
      "type": "string"
    },
    "blocco_contenuto": {
      "type": "dynamiczone",
      "components": [
        "layout.articolo",
        "layout.titolo",
        "config.tabella"
      ]
    },
    "barra_superiore": {
      "type": "dynamiczone",
      "components": [
        "layout.titolo",
        "layout.articolo"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "titolo"
    },
    "pagina": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::pagina.pagina"
    },
    "composizioneTitolo": {
      "type": "customField",
      "customField": "plugin::multi-select.multi-select",
      "default": "[titolo]",
      "options": [
        "titolo",
        "data",
        "comune",
        "fonte",
        "fontePrefisso"
      ]
    }
  }
}
